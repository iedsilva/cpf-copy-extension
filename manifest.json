{
    "manifest_version": 3,
    "name": "CPF Generator",
    "version": "1.0",
    "description": "Generate a CPF and copy to clipboard",
    "permissions": ["clipboardWrite"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon16.png",
        "128": "icon16.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon16.png",
      "128": "icon16.png"
    }
  }
  